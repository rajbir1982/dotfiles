[user]
	name = Tracy Phillips
	email = tracphil@outlook.com

[core]
  	editor = subl -n -w
  	excludesfile = ~/.gitignore_global
  	autocrlf = false
  	safecrlf = true
  	quotepath = false
  	whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
  	ignorecase = false

[branch]
  	autosetuprebase = always

[log]
  	date = short
  	mailmap = true

[format]
  	pretty = '%C(yellow)%h%Creset %C(magenta)%cd%Creset %d %s'

[github]
  	user = reavon

[commit]
  	template = ~/.gitmessage

[color]
  	branch = auto
  	decorate = auto
  	diff = auto
  	grep = auto
  	interactive = auto
  	pager = true
  	showbranch = auto
  	status = auto
  	ui = auto

[color "branch"]
  	current = yellow reverse
  	local   = yellow
  	remote  = green

[color "diff"]
  	meta = 8
  	func = 6
  	frag = 8
  	old = 9
  	context = 7
  	new = 10

[color "status"]
  	header = 8
  	added = 10
  	changed = 9
  	untracked = 3
  	branch = 0 5

[color "grep"]
  	match = 33

[transfer]
  	fsckobjects = true

[fetch]
  	fsckobjects = true

[receive]
  	advertisepushoptions = true

[init]
  	defaultBranch = main
